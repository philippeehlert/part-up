{
    "parser": "babel-eslint",
    "extends": "google",
    "plugins": ["meteor", "babel"],
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "rules": {
        "strict": 0,
        "object-curly-spacing": 0,
        "require-jsdoc": 0,
        "comma-dangle": [2, "always-multiline"],
        "indent": [2, 2, {"SwitchCase": 1}],
        "max-len": 0,
        "camelcase": 0,
        "valid-jsdoc": 0,
        "guard-for-in": 0,
        "new-cap": 0,
        "padded-blocks": 0,
        "keyword-spacing": 2,
        "id-length": 0,
        "id-blacklist": 0,
        "no-process-env": 0,
        "no-empty-function": 0,
        "no-extra-parens": 0,
        "no-extra-semi": 0,
        "no-undefined": 0,
        "no-implicit-coercion": [1, { "boolean": false }],
        "no-underscore-dangle": 0,
        "no-return-assign": ["error", "except-parens"],
        "arrow-body-style": 0,
        "no-invalid-this": 0,
        "babel/no-invalid-this": 1,
        "babel/semi": 1
    }
}
