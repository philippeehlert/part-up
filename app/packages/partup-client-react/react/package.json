{
  "name": "part-up-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "classnames": "^2.2.5",
    "emoji-js": "^3.4.0",
    "get-form-data": "^1.2.5",
    "i18next": "^10.0.7",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "marked": "^0.3.12",
    "moment": "^2.19.1",
    "node-sass-chokidar": "^0.0.3",
    "npm-run-all": "^4.1.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-router-dom": "^4.2.2",
    "react-scripts-ts": "2.8.0",
    "react-web-meteor": "https://github.com/andreleon/react-web-meteor.git",
    "spin": "^0.0.1"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts-ts start",
    "start": "DEBUG=info REACT_APP_DEV=1 PORT=4000 npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "generate-icons": "node_modules/.bin/babel-node generate-icons.js"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.3",
    "@types/i18next": "^8.4.2",
    "@types/jest": "^21.1.4",
    "@types/jquery": "^3.2.15",
    "@types/lodash": "^4.14.78",
    "@types/moment": "^2.13.0",
    "@types/node": "^8.0.46",
    "@types/prop-types": "^15.5.2",
    "@types/react": "^16.0.18",
    "@types/react-dom": "^16.0.2",
    "@types/react-router": "^4.0.16",
    "@types/react-router-dom": "^4.0.8",
    "@types/react-select": "^1.0.59",
    "@types/showdown": "^1.7.2",
    "babel-cli": "^6.26.0",
    "fs-extra": "^4.0.2",
    "svgo": "^0.7.2",
    "tslint": "^5.8.0",
    "tslint-eslint-rules": "^4.1.1"
  }
}
