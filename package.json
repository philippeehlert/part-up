    "gulp-load-plugins": "^1.2.4",
    "gulp-notify": "^3.0.0",
    "gulp-plumber": "^1.1.0",
    "gulp-tag-version": "^1.3.0",
    "gulp-util": "^3.0.7",
    "json2yaml": "^1.1.0",
    "map-stream": "^0.0.7",
