    "bluebird": "^3.3.5",
    "chimp": "^0.49.0",
    "commitizen": "^2.8.2",
    "conventional-changelog": "^1.1.4",
    "conventional-changelog-cli": "^1.2.0",
    "conventional-github-releaser": "^1.1.11",
